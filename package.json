{
  "name": "kickjump-monorepo",
  "private": true,
  "description": "Your toolkit for financially sustainable open source development.",
  "license": "BSD-3-Clause",
  "contributors": [],
  "type": "module",
  "scripts": {
    "build": "run-p build:packages typecheck",
    "build:kickjump.com": "pnpm --dir apps/kickjump.com build",
    "build:packages": "pnpm monots build && pnpm run --recursive --filter ./packages postbuild",
    "changeset": "changeset",
    "checks": "run-s lint typecheck test test:kickjump.com test:db build:kickjump.com build:packages",
    "clean": "git clean -xfd -e '*.env'",
    "compose:down": "docker compose down",
    "compose:up": "docker compose up",
    "dev": "pnpm monots prepare",
    "fix": "run-s -c fix:repo fix:build fix:es fix:prettier",
    "fix:build": "pnpm monots fix",
    "fix:es": "pnpm lint:es -- --fix",
    "fix:prettier": "pnpm run:prettier -- --write",
    "fix:repo": "manypkg fix",
    "preinstall": "pnpm scripts symlink:root && pnpm scripts check:pnpm",
    "postinstall": "run-s dev prisma:generate",
    "lint": "run-s lint:\\*",
    "lint:build": "pnpm monots check",
    "lint:es": "eslint --ext=.tsx,.ts,.js .",
    "lint:prettier": "pnpm run:prettier -- --check",
    "lint:repo": "manypkg check",
    "monots": "monots",
    "patch": "patch-package --patch-dir .monots/patches",
    "prepare": "husky install .monots/.husky || true",
    "prisma": "pnpm --dir packages/kickjump__prisma prisma",
    "prisma:generate": "pnpm --dir packages/kickjump__prisma generate",
    "prisma:push:local": "pnpm --dir packages/kickjump__prisma push:local",
    "prisma:studio": "pnpm --dir packages/kickjump__prisma studio",
    "prisma:studio:local": "pnpm --dir packages/kickjump__prisma studio:local",
    "prerelease": "pnpm build",
    "release": "changeset publish",
    "reset": "pnpm clean; rm pnpm-lock.yaml; pnpm symlink:root; pnpm i -r;",
    "run:prettier": "prettier --ignore-path .eslintignore \"**/*.{js,jsx,ts,tsx,md,mdx,json,html,css,yml,yaml,graphql}\"",
    "scripts": "pnpm --dir .monots/scripts run",
    "test": "vitest run",
    "test:db": "pnpm --dir tests test:db",
    "test:kickjump.com": "pnpm --dir tests/kickjump.com test",
    "test:watch": "vitest watch",
    "ts": "node --experimental-specifier-resolution=node --loader ts-node/esm",
    "typecheck": "tsc --build --force",
    "update:deps": "pnpm update --latest --recursive -i",
    "update:workspace": "pnpm up -r --workspace \"packages/*\"",
    "vercel": "vercel",
    "version:changeset": "changeset version",
    "version:ci": "run-s version:changeset version:date fix:repo update:workspace fix:prettier version:lockfile",
    "version:date": "pnpm scripts update:changelog:dates",
    "version:lockfile": "pnpm install --lockfile-only",
    "version:pr": "pnpm scripts delete:changeset:property"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.2",
    "@changesets/cli": "^2.19.0",
    "@manypkg/cli": "^0.19.1",
    "@manypkg/get-packages": "^1.1.3",
    "@milahu/patch-package-with-pnpm-support": "6.4.8",
    "@monots/cli": "^0.11.0",
    "@monots/tsconfig": "^0.1.0",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^17.0.13",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@vitejs/plugin-react": "^1.1.4",
    "eslint": "^8.7.0",
    "eslint-config-monots": "^0.6.0",
    "eslint-config-next": "12.0.9",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-testing-library": "^5.0.4",
    "happy-dom": "^2.28.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-config-monots": "^0.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "vercel": "^23.1.2",
    "vite": "^2.7.13",
    "vitest": "^0.2.5"
  },
  "engines": {
    "node": ">=16.9",
    "pnpm": ">=6"
  },
  "monots": {
    "packages": [
      ".monots/scripts",
      "apps/*",
      "packages/*",
      "tests",
      "tests/kickjump.com"
    ],
    "tsconfigPath": "./.monots/symlink/tsconfig.json",
    "baseTsconfig": "./.monots/tsconfig.base.json",
    "tsconfig": {
      "ts-node": {
        "transpileOnly": true,
        "compilerOptions": {
          "esModuleInterop": true,
          "target": "ESNext"
        }
      }
    }
  },
  "packageManager": "pnpm@6.28.0",
  "pnpm": {
    "overrides": {
      "bn.js": "^5.2.0",
      "@solana/spl-token": "^0.1.8",
      "@solana/wallet-adapter-react": "^0.15.2",
      "@solana/wallet-adapter-wallets": "^0.14.2",
      "@solana/wallet-adapter-base": "^0.9.2",
      "@project-serum/anchor": "^0.20.1"
    }
  }
}
