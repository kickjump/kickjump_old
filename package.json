{
  "name": "kickjump-monorepo",
  "private": true,
  "description": "Your toolkit for financially sustainable open source development.",
  "license": "BSD-3-Clause",
  "contributors": [],
  "type": "module",
  "scripts": {
    "bootstrap": "corepack enable && pnpm i && run-p setup:rust setup:anchor setup:playwright",
    "build": "run-p build:packages typecheck",
    "build:kickjump.com": "pnpm --dir apps/kickjump.com build",
    "build:packages": "pnpm monots build && pnpm run --recursive --filter ./packages postbuild",
    "changeset": "changeset",
    "checks": "run-s lint typecheck test test:kickjump.com test:db build:kickjump.com build:packages",
    "clean": "git clean -xfd -e '*.env' -e '.vscode' -e 'Anchor.toml' -e rust-toolchain.toml -e rustfmt.toml",
    "dependencies": "run-p update:\\*",
    "dev": "pnpm monots prepare",
    "dev:kickjump.com": "pnpm --dir apps/kickjump.com dev",
    "fix": "run-s -c fix:audit:packages fix:audit:crates fix:repo fix:build fix:es fix:recursive fix:rustfmt fix:prettier",
    "fix:audit:crates": "cargo audit fix",
    "fix:audit:packages": "pnpm scripts fix:audit",
    "fix:build": "pnpm monots fix",
    "fix:es": "pnpm lint:es --fix",
    "fix:prettier": "pnpm run:prettier --write",
    "fix:recursive": "pnpm run --recursive fix",
    "fix:repo": "manypkg fix",
    "fix:rustfmt": "cargo +nightly fmt",
    "preinstall": "pnpm scripts symlink:root && pnpm scripts check:pnpm",
    "postinstall": "run-s dev",
    "lint": "run-s lint:\\*",
    "lint:audit:crates": "cargo audit",
    "lint:audit:packages": "pnpm audit",
    "lint:build": "pnpm monots check",
    "lint:es": "ESLINT_FULL_CHECK=1 eslint --ext=.tsx,.ts,.js,.svelte .",
    "lint:packages": "pnpm run -r check",
    "lint:prettier": "pnpm run:prettier --check",
    "lint:repo": "manypkg check",
    "lint:rustfmt": "cargo +nightly fmt --check",
    "prepare": "husky install .monots/.husky || true",
    "refresh": "pnpm clean; pnpm i -r;",
    "prerelease": "pnpm build",
    "release": "changeset publish",
    "reset": "pnpm clean; rm pnpm-lock.yaml; pnpm i -r",
    "run:prettier": "prettier --ignore-path .eslintignore \"**/*.{js,jsx,ts,tsx,md,mdx,json,html,css,yml,yaml,graphql}\"",
    "scripts": "pnpm --dir .monots/scripts run",
    "setup:anchor": "pnpm scripts setup:anchor && anchor build",
    "setup:playwright": "playwright install",
    "setup:rust": "cargo upgrade -h || cargo install cargo-edit; cargo audit fix -h || cargo install cargo-audit --features=fix",
    "start:kickjump.com": "pnpm --dir apps/kickjump.com start",
    "test": "run-p -c test:kickjump.com test:packages test:db",
    "test:db": "pnpm --dir tests test:db",
    "test:kickjump.com": "pnpm --dir tests test:kickjump.com",
    "test:kickjump.com:watch": "E2E_QUICK=1 chokidar \"tests/kickjump.com/**/*.ts\" \"packages/kickjump__playwright/src/**/*.ts\" \"apps/kickjump.com/src/**/*.{ts,svelte}\" -c \"pnpm test:kickjump.com\" --ignore \"node_modules/**\" --initial --silent",
    "test:packages": "vitest run",
    "test:playwright": "pnpm run -r test:playwright",
    "test:watch:packages": "vitest watch",
    "typecheck": "tsc --build --force",
    "update:crates": "cargo upgrade --workspace",
    "update:deps": "pnpm update --latest --recursive -i",
    "version:changeset": "changeset version",
    "version:ci": "run-s version:changeset version:date fix:repo fix:prettier version:lockfile",
    "version:date": "pnpm scripts update:changelog:dates",
    "version:lockfile": "pnpm install --lockfile-only",
    "version:pr": "pnpm scripts delete:changeset:property"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.5",
    "@changesets/cli": "^2.23.2",
    "@cloudflare/wrangler": "^1.19.12",
    "@manypkg/cli": "^0.19.1",
    "@manypkg/get-packages": "^1.1.3",
    "@monots/cli": "^0.13.1",
    "@monots/tsconfig": "^0.2.0",
    "@parcel/css-cli": "^1.11.2",
    "@sveltejs/vite-plugin-svelte": "1.0.1",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.2.1",
    "@types/node": "^18.0.4",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitest/ui": "^0.18.0",
    "autoprefixer": "^10.4.7",
    "chokidar-cli": "^3.0.0",
    "cpy-cli": "^4.1.0",
    "daisyui": "^2.19.0",
    "eslint": "^8.19.0",
    "eslint-config-monots": "^0.11.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-playwright": "^0.9.0",
    "happy-dom": "^6.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "localtunnel": "^2.0.2",
    "msw": "^0.44.0",
    "ngrok": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.23.3",
    "playwright-watch": "^1.3.23",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "prettier-config-monots": "^0.2.2",
    "svelte": "^3.49.0",
    "tailwindcss": "^3.1.6",
    "tailwindcss-animate": "^1.0.5",
    "tsconfig-paths": "^4.0.0",
    "tsx": "^3.8.0",
    "typescript": "4.8.0-beta",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.18.0",
    "wasm-pack": "^0.10.3",
    "wrangler": "^2.0.19"
  },
  "packageManager": "pnpm@7.5.2",
  "engines": {
    "node": ">=16.9",
    "pnpm": ">=6"
  },
  "monots": {
    "packages": [
      ".monots/scripts",
      "apps/kickjump.com",
      "packages/*",
      "tests",
      "tests/kickjump.com"
    ],
    "tsconfigPath": "./tsconfig.json",
    "baseTsconfig": "./.monots/tsconfig.base.json"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/css-cli",
      "wasm-pack"
    ],
    "overrides": {
      "@solana/web3.js@<1.48.0": "^1.48.0",
      "@project-serum/anchor@<0.25.0": "^0.25.0",
      "cross-fetch": ">=3.1.5",
      "node-forge@<1.3.0": ">=1.3.0",
      "glob-parent@<5.1.2": ">=5.1.2",
      "node-fetch@<2.6.7": ">=2.6.7",
      "node-forge@<1.0.0": ">=1.0.0",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "async@<2.6.4": ">=2.6.4",
      "got@<11.8.5": "^11.8.5"
    },
    "patchedDependencies": {
      "fast-sort@3.2.0": ".monots/patches/fast-sort@3.2.0.patch",
      "@sveltejs/kit@1.0.0-next.372": ".monots/patches/@sveltejs__kit@1.0.0-next.372.patch",
      "eslint-config-monots@0.11.1": ".monots/patches/eslint-config-monots@0.11.1.patch"
    }
  }
}
