{
  "name": "kickjump-monorepo",
  "private": true,
  "description": "Your toolkit for financially sustainable open source development.",
  "license": "BSD-3-Clause",
  "contributors": [],
  "type": "module",
  "scripts": {
    "bootstrap": "corepack enable && pnpm i && run-p setup:rust setup:anchor",
    "build": "run-p build:packages typecheck",
    "build:kickjump.com": "pnpm --dir apps/kickjump.com build",
    "build:packages": "pnpm monots build && pnpm run --recursive --filter ./packages postbuild",
    "changeset": "changeset",
    "checks": "run-s lint typecheck test test:kickjump.com test:db build:kickjump.com build:packages",
    "clean": "git clean -xfd -e '*.env' -e '.vscode' -e 'Anchor.toml'",
    "compose:down": "docker compose down",
    "compose:up": "docker compose up",
    "dev": "pnpm monots prepare",
    "fix": "run-s -c fix:repo fix:build fix:es fix:prettier",
    "fix:audit": "pnpm scripts fix:audit",
    "fix:build": "pnpm monots fix",
    "fix:es": "pnpm lint:es --fix",
    "fix:idl": "pnpm run --recursive fix:idl",
    "fix:prettier": "pnpm run:prettier --write",
    "fix:repo": "manypkg fix",
    "preinstall": "pnpm scripts symlink:root && pnpm scripts check:pnpm",
    "postinstall": "run-s dev prisma:generate",
    "lint": "run-s lint:\\*",
    "lint:audit": "pnpm audit",
    "lint:build": "pnpm monots check",
    "lint:es": "eslint --ext=.tsx,.ts,.js,.svelte .",
    "lint:prettier": "pnpm run:prettier --check",
    "lint:repo": "manypkg check",
    "prepare": "husky install .monots/.husky || true",
    "prisma": "pnpm --dir packages/kickjump__prisma prisma",
    "prisma:generate": "pnpm --dir packages/kickjump__prisma generate",
    "prisma:push:local": "pnpm --dir packages/kickjump__prisma push:local",
    "prisma:studio": "pnpm --dir packages/kickjump__prisma studio",
    "prisma:studio:local": "pnpm --dir packages/kickjump__prisma studio:local",
    "refresh": "pnpm clean; pnpm i -r;",
    "prerelease": "pnpm build",
    "release": "changeset publish",
    "reset": "pnpm clean; rm pnpm-lock.yaml; pnpm i -r;",
    "run:prettier": "prettier --ignore-path .eslintignore \"**/*.{js,jsx,ts,tsx,md,mdx,json,html,css,yml,yaml,graphql}\"",
    "scripts": "pnpm --dir .monots/scripts run",
    "setup:anchor": "pnpm scripts setup:anchor && anchor build",
    "setup:playwright": "playwright install-deps",
    "setup:rust": "cargo upgrade -h || cargo install cargo-edit",
    "start:kickjump.com": "pnpm --dir apps/kickjump.com start",
    "test": "run-p -c test:kickjump.com test:packages test:db",
    "test:db": "pnpm --dir tests test:db",
    "test:kickjump.com": "pnpm --dir tests/kickjump.com test",
    "test:packages": "vitest run",
    "test:watch:packages": "vitest watch",
    "ts": "node --experimental-specifier-resolution=node --loader ts-node/esm",
    "typecheck": "tsc --build --force",
    "update:deps": "pnpm update --latest --recursive -i",
    "update:workspace": "pnpm up -r --workspace \"packages/*\"",
    "vercel": "vercel",
    "version:changeset": "changeset version",
    "version:ci": "run-s version:changeset version:date fix:repo update:workspace fix:prettier version:lockfile",
    "version:date": "pnpm scripts update:changelog:dates",
    "version:lockfile": "pnpm install --lockfile-only",
    "version:pr": "pnpm scripts delete:changeset:property"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "@cloudflare/wrangler": "^1.19.12",
    "@gokiprotocol/walletkit": "^1.6.8",
    "@manypkg/cli": "^0.19.1",
    "@manypkg/get-packages": "^1.1.3",
    "@monots/cli": "^0.12.4",
    "@monots/tsconfig": "^0.1.0",
    "@parcel/css-cli": "^1.8.1",
    "@playwright/experimental-ct-svelte": "0.0.6",
    "@playwright/test": "1.22.0-alpha-apr-27-2022",
    "@remix-run/eslint-config": "^1.4.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.42",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.1.1",
    "@types/node": "^17.0.29",
    "@types/tailwindcss": "^3.0.10",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@vitest/ui": "latest",
    "autoprefixer": "^10.4.5",
    "daisyui": "^2.14.2",
    "eslint": "^8.14.0",
    "eslint-config-monots": "^0.11.0",
    "eslint-config-next": "12.1.5",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-svelte3": "^3.4.1",
    "eslint-plugin-testing-library": "^5.3.1",
    "happy-dom": "^3.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "npm-run-all": "^4.1.5",
    "playwright": "1.22.0-alpha-apr-27-2022",
    "playwright-watch": "^1.3.23",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-load-config": "^3.1.4",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.4",
    "prettier": "^2.6.2",
    "prettier-config-monots": "^0.2.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "svelte": "^3.47.0",
    "tailwindcss": "^3.0.24",
    "tailwindcss-animate": "^1.0.5",
    "ts-node": "^10.7.0",
    "typescript": "4.7.0-beta",
    "vitest": "^0.10.0",
    "wireit": "^0.2.0"
  },
  "packageManager": "pnpm@7.0.0-rc.9",
  "engines": {
    "node": ">=16.9",
    "pnpm": ">=6"
  },
  "monots": {
    "packages": [
      ".monots/scripts",
      "apps/kickjump.com",
      "packages/*",
      "tests",
      "tests/kickjump.com"
    ],
    "tsconfigPath": "./.monots/symlink/tsconfig.json",
    "baseTsconfig": "./.monots/tsconfig.base.json",
    "tsconfig": {
      "ts-node": {
        "transpileOnly": true,
        "compilerOptions": {
          "esModuleInterop": true,
          "target": "ESNext"
        }
      }
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/css-cli"
    ],
    "overrides": {
      "idb-keyval": ">=6.1.0",
      "@solana/web3.js": ">=1.41.0",
      "@project-serum/anchor": ">=0.24.2",
      "cross-fetch": ">=3.1.5",
      "node-forge@<1.3.0": ">=1.3.0",
      "glob-parent@<5.1.2": ">=5.1.2",
      "node-fetch@<2.6.7": ">=2.6.7",
      "node-forge@<1.0.0": ">=1.0.0",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "async@<2.6.4": ">=2.6.4"
    }
  }
}
