{
  "name": "e2e-tests",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test:anchor": "vitest run --config ./anchor.config.ts",
    "test:anchor:watch": "vitest --config ./anchor.config.ts",
    "test:db": "vitest run --config ./db.config.ts",
    "test:db:watch": "DATABASE_URL=mysql://root:kickjump@localhost:3307/kickjump vitest --config ./db.config.ts",
    "test:kickjump.com": "playwright test",
    "trace:kickjump.com": "playwright show-trace"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.4.3",
    "@faker-js/faker": "^7.3.0",
    "@kickjump/db": "^0.0.1",
    "@kickjump/mocks": "^0.0.0",
    "@kickjump/playwright": "^0.0.0",
    "@playwright/test": "^1.24.1",
    "@project-serum/anchor": "0.25.0",
    "chalk": "^5.0.1",
    "delay": "^5.0.0",
    "execa": "^6.1.0",
    "jest-process-manager": "^0.3.1",
    "load-json-file": "^7.0.1",
    "p-retry": "^5.1.1",
    "solana-credentials": "^0.0.1"
  },
  "monots": {
    "tsconfigs": {
      "src": false,
      "": {
        "compilerOptions": {
          "exactOptionalPropertyTypes": true
        },
        "include": [
          "anchor",
          "db",
          "setup"
        ]
      }
    },
    "mode": "app"
  }
}
