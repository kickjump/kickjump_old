{
  "name": "solana-credentials",
  "version": "0.0.1",
  "description": "Manage non-transferrable credentials for solana accounts.",
  "keywords": [],
  "license": "AGPLv3",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./index.js": {
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "fix:idl": "cp ../../target/types/solana_credentials.ts src/generated.ts && eslint --fix src/generated.ts && prettier --write src/generated.ts"
  },
  "dependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts"
    ],
    "tsconfigs": {},
    "mode": "library",
    "externalModules": []
  }
}
