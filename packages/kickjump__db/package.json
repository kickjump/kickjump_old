{
  "name": "@kickjump/db",
  "version": "0.0.1",
  "private": true,
  "description": "Internal database client with shared methods",
  "keywords": [],
  "license": "AGPLv3",
  "contributors": [],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./seed": {
      "types": "./dist/seed.d.ts",
      "import": "./dist/seed.esm.js"
    },
    "./seed.js": {
      "types": "./dist/seed.d.ts",
      "import": "./dist/seed.esm.js"
    },
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "seed"
  ],
  "scripts": {
    "seed": "EDGEDB_INSTANCE=\"kjdb\" tsx scripts/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^7.4.0",
    "@kickjump/edgedb": "^0.0.0",
    "@kickjump/svelte-auth": "^0.0.0",
    "@kickjump/types": "^0.0.0",
    "edgedb": "0.22.1",
    "is-what": "^4.1.7",
    "tiny-invariant": "^1.2.0",
    "ts-extras": "^0.11.0",
    "type-fest": "^2.19.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.13"
  },
  "peerDependencies": {
    "@types/node": "*"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "seed.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ],
          "allowJs": true,
          "noEmit": false
        }
      },
      "scripts": {}
    },
    "mode": "library",
    "externalModules": []
  }
}
