{
  "name": "@kickjump/db",
  "version": "0.0.1",
  "private": true,
  "description": "Internal database client with shared methods",
  "keywords": [],
  "license": "MIT",
  "contributors": [],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./index.js": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@kickjump/prisma": "^0.0.1",
    "@swc/helpers": "^0.3.13",
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "tiny-invariant": "^1.2.0",
    "type-fest": "^2.12.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "seed.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "module": "NodeNext"
        }
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
