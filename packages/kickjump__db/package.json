{
  "name": "@kickjump/db",
  "version": "0.0.1",
  "private": true,
  "description": "Internal database client with shared methods",
  "keywords": [],
  "license": "MIT",
  "contributors": [],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./seed": {
      "types": "./dist/seed.d.ts",
      "import": "./dist/seed.esm.js"
    },
    "./seed.js": {
      "types": "./dist/seed.d.ts",
      "import": "./dist/seed.esm.js"
    },
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "seed"
  ],
  "scripts": {
    "fix": "edgeql-js --target ts --output-dir src/generated --force-overwrite",
    "migrate": "edgedb migration create && edgedb migrate && pnpm fix",
    "seed": "tsx dbschema/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "@kickjump/svelte-auth": "^0.0.0",
    "edgedb": "0.21.0-alpha.3",
    "tiny-invariant": "^1.2.0",
    "type-fest": "^2.13.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "seed.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [],
          "noEmit": true
        }
      },
      "dbschema": {}
    },
    "mode": "library",
    "externalModules": []
  }
}
