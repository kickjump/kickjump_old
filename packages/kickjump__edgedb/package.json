{
  "name": "@kickjump/edgedb",
  "version": "0.0.0",
  "private": true,
  "description": "The automated edgedb bindings",
  "keywords": [],
  "license": "MIT",
  "contributors": [],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "seed"
  ],
  "scripts": {
    "build": "pnpm run dev",
    "db:create": "edgedb instance create",
    "db:init": "edgedb project init --non-interactive || edgedb project init --server-instance kickjump__edgedb --non-interactive",
    "db:remove": "edgedb instance destroy --force --instance",
    "dev": "edgeql-js --target esm --output-dir dist --force-overwrite",
    "migrate": "run-s migrate:create migrate:run dev",
    "migrate:create": "edgedb migration create",
    "migrate:run": "edgedb migrate",
    "prepare": "pnpm run dev"
  },
  "dependencies": {
    "edgedb": "0.21.0-beta.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "tsconfigs": {
      "src": false
    },
    "mode": "app"
  }
}
