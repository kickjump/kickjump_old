{
  "name": "@kickjump/edgedb",
  "version": "0.0.0",
  "private": true,
  "description": "The automated edgedb bindings",
  "keywords": [],
  "license": "MIT",
  "contributors": [],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs"
    },
    "./types.js": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "seed"
  ],
  "scripts": {
    "build": "pnpm run dev",
    "db:create": "edgedb instance create",
    "db:init": "edgedb project init --non-interactive || edgedb project init --server-instance kickjump__edgedb --non-interactive",
    "db:remove": "edgedb instance destroy --force --instance",
    "dev": "run-s generate:esm types:copy types:build",
    "generate:esm": "edgeql-js --target=esm --output-dir dist --force-overwrite",
    "maybe_fix": "edgeql-js --target ts --output-dir tmp --force-overwrite && tsx scripts/patch-edgedb-gen.ts && rm -rf src/config.json src/*.ts src/modules src/syntax && mv tmp/* src/",
    "migrate": "run-s migrate:create migrate:run dev",
    "migrate:create": "edgedb migration create",
    "migrate:run": "edgedb migrate",
    "prepare": "pnpm run dev",
    "types:build": "esbuild tmp/types.ts --target=node16 --format=esm > dist/types.mjs",
    "types:copy": "rm -rf tmp && mkdir tmp && cp dist/types.d.ts tmp/types.ts"
  },
  "dependencies": {
    "edgedb": "^0.21.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "tsconfigs": {
      "src": false
    },
    "mode": "app"
  }
}
