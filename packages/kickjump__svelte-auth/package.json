{
  "name": "@kickjump/svelte-auth",
  "version": "0.0.0",
  "description": "Cookie based authentication provider inspired by remix-auth",
  "keywords": [],
  "license": "AGPLv3",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "browser": "./dist/client.browser.esm.js",
      "import": "./dist/client.esm.js"
    },
    "./client.js": {
      "types": "./dist/client.d.ts",
      "browser": "./dist/client.browser.esm.js",
      "import": "./dist/client.esm.js"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "client",
    "dist"
  ],
  "dependencies": {
    "is-what": "^4.1.7",
    "nanoid": "^4.0.0",
    "path-to-regexp": "^6.2.1",
    "superjson": "^1.9.1",
    "svelte-kit-cookie-session": "^3.1.0",
    "tiny-invariant": "^1.2.0",
    "ts-extras": "^0.11.0",
    "type-fest": "^2.19.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "1.0.0-next.442",
    "svelte": "^3.49.0",
    "vite": "^3.0.9"
  },
  "peerDependencies": {
    "@sveltejs/kit": "next",
    "svelte": ">=3.44.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "client.ts",
      "providers.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "@sveltejs/kit"
          ],
          "noEmit": true
        }
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
