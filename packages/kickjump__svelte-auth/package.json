{
  "name": "@kickjump/svelte-auth",
  "version": "0.0.0",
  "description": "Cookie based authentication provider inspired by remix-auth",
  "keywords": [],
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "browser": "./dist/client.browser.esm.js",
      "import": "./dist/client.esm.js",
      "types": "./dist/client.d.ts"
    },
    "./client.js": {
      "browser": "./dist/client.browser.esm.js",
      "import": "./dist/client.esm.js",
      "types": "./dist/client.d.ts"
    },
    "./index.js": {
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "client",
    "dist"
  ],
  "dependencies": {
    "@swc/helpers": "^0.3.8",
    "nanoid": "^3.3.3",
    "path-to-regexp": "6.2.0",
    "superstruct": "^0.15.4",
    "svelte-kit-cookie-session": "^2.1.3",
    "tiny-invariant": "^1.2.0",
    "type-fest": "^2.12.2"
  },
  "devDependencies": {
    "@sveltejs/kit": "next"
  },
  "peerDependencies": {
    "@sveltejs/kit": "next"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "client.ts",
      "providers.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "@sveltejs/kit"
          ],
          "noEmit": true
        }
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
