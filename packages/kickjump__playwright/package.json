{
  "name": "@kickjump/playwright",
  "version": "0.0.0",
  "private": true,
  "description": "Internal playwright testing tools.",
  "keywords": [],
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./index.js": {
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./svelte": {
      "import": "./dist/svelte.esm.js",
      "types": "./dist/svelte.d.ts"
    },
    "./svelte.js": {
      "import": "./dist/svelte.esm.js",
      "types": "./dist/svelte.d.ts"
    },
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "svelte"
  ],
  "dependencies": {
    "@playwright/test": "^1.24.0",
    "delay": "^5.0.0",
    "playwright-core": "^1.24.0",
    "undici": "^5.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.6.1"
  },
  "peerDependencies": {
    "@types/node": "*"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "svelte.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ]
        }
      }
    },
    "mode": "library",
    "externalModules": []
  }
}
