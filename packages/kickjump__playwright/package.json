{
  "name": "@kickjump/playwright",
  "version": "0.0.0",
  "private": true,
  "description": "Internal playwright testing tools.",
  "keywords": [],
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./index.js": {
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./svelte": {
      "import": "./dist/svelte.esm.js",
      "types": "./dist/svelte.d.ts"
    },
    "./svelte.js": {
      "import": "./dist/svelte.esm.js",
      "types": "./dist/svelte.d.ts"
    },
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "svelte"
  ],
  "dependencies": {
    "@playwright-testing-library/test": "^4.2.1",
    "@playwright/test": "^1.22.2",
    "@swc/helpers": "^0.4.0",
    "delay": "^5.0.0",
    "playwright-core": "^1.22.2",
    "playwright-testing-library": "^4.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "svelte.ts"
    ],
    "tsconfigs": {},
    "mode": "library",
    "externalModules": []
  }
}
