{
  "name": "@kickjump/query",
  "version": "0.0.0",
  "description": "A svelte fork of the '@tanstack/react-query' library",
  "keywords": [],
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./dist/Hydrate": "./dist/Hydrate.svelte",
    "./dist/Hydrate.svelte": "./dist/Hydrate.svelte",
    "./dist/QueryClientProvider": "./dist/QueryClientProvider.svelte",
    "./dist/QueryClientProvider.svelte": "./dist/QueryClientProvider.svelte",
    "./index.js": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "svelte.js"
  ],
  "scripts": {
    "build": "tsx scripts/build.ts",
    "dev": "tsx scripts/dev.ts"
  },
  "dependencies": {
    "@tanstack/query-core": "^4.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "svelte": "^3.49.0"
  },
  "peerDependencies": {
    "svelte": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts"
    ],
    "tsconfigs": {
      "tests": {
        "compilerOptions": {
          "types": [
            "svelte"
          ]
        }
      },
      "src": {
        "compilerOptions": {
          "types": [],
          "noEmit": true
        }
      }
    },
    "mode": "library",
    "externalModules": [],
    "extraExports": {
      "./dist/QueryClientProvider": "./dist/QueryClientProvider.svelte",
      "./dist/QueryClientProvider.svelte": "./dist/QueryClientProvider.svelte",
      "./dist/Hydrate": "./dist/Hydrate.svelte",
      "./dist/Hydrate.svelte": "./dist/Hydrate.svelte"
    }
  }
}
