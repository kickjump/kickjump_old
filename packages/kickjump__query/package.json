{
  "name": "@kickjump/query",
  "version": "0.0.0",
  "description": "A svelte fork of the '@tanstack/react-query' library",
  "keywords": [],
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./index.js": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.esm.js",
      "import": "./dist/index.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.browser.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@tanstack/query-core": "^4.0.0-beta.1",
    "@types/use-sync-external-store": "^0.0.3",
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@testing-library/svelte": "^3.2.1",
    "@tsconfig/svelte": "^3.0.0",
    "svelte": "^3.49.0"
  },
  "peerDependencies": {
    "svelte": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts"
    ],
    "tsconfigs": {
      "tests": {
        "compilerOptions": {
          "types": [
            "svelte"
          ]
        }
      }
    },
    "mode": "library",
    "externalModules": [],
    "extraExports": {}
  }
}
