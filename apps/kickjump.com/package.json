{
  "name": "kickjump.com",
  "private": true,
  "description": "kickjump.com website",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s build:\\*",
    "build:css": "run-s css:generate css:minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build",
    "css:generate": "unocss \"app/**/*.{ts,tsx}\" --out-file=app/uno.css",
    "css:minify": "parcel-css --minify --nesting --bundle --sourcemap app/uno.css -o app/uno.css",
    "dev": "run-p dev:\\*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:css": "pnpm css:generate --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "dependencies": {
    "@kickjump/components": "^0.0.1",
    "@kickjump/db": "^0.0.1",
    "@kickjump/tokens": "^0.0.1",
    "@octokit/app": "^12.0.5",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/core": "^3.6.0",
    "@octokit/openapi-types": "^11.2.0",
    "@prisma/client": "^3.12.0",
    "@remix-run/express": "^1.3.5",
    "@remix-run/node": "^1.3.5",
    "@remix-run/react": "^1.3.5",
    "@remix-run/server-runtime": "^1.3.5",
    "@solana/wallet-adapter-react": "^0.15.4",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "bn.js": "^5.2.0",
    "bs58": "^5.0.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "morgan": "^1.10.0",
    "octokit": "^1.7.1",
    "poly-buffer": "npm:buffer@^6.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hot-toast": "^2.2.0",
    "superjson": "^1.8.1",
    "superstruct": "^0.15.4",
    "tiny-invariant": "^1.2.0",
    "ts-extras": "^0.10.1",
    "tweetnacl": "^1.0.3",
    "type-fest": "^2.12.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@remix-run/dev": "^1.3.5",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@unocss/cli": "^0.30.12",
    "@unocss/preset-attributify": "^0.30.12",
    "@unocss/preset-icons": "^0.30.12",
    "@unocss/preset-uno": "^0.30.12",
    "@unocss/preset-web-fonts": "^0.30.12",
    "@unocss/reset": "^0.30.12",
    "autoprefixer": "^10.4.4",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.29",
    "postcss": "^8.4.12",
    "unocss": "^0.30.12"
  },
  "engines": {
    "node": ">=14"
  },
  "monots": {
    "mode": "app",
    "externalModules": [],
    "tsconfigs": {
      "src": false,
      "": {
        "include": [
          "remix.env.d.ts",
          "**/*.ts",
          "**/*.tsx"
        ],
        "compilerOptions": {
          "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
          ],
          "isolatedModules": true,
          "jsx": "react-jsx",
          "module": "CommonJS",
          "moduleResolution": "node",
          "target": "ESNext",
          "baseUrl": ".",
          "paths": {
            "~/*": [
              "./app/*"
            ]
          },
          "skipLibCheck": true,
          "noEmit": true
        }
      }
    }
  }
}
