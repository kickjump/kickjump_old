  "monots": {
    "mode": "app",
    "externalModules": [],
    "tsconfigs": {
      "src": false,
      "": {
        "compilerOptions": {
          "noEmit": true,
          "declaration": false,
          "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
          ],
          "preserveValueImports": true,
          "isolatedModules": true,
          "rootDirs": [
            "./",
            "./.svelte-kit/types"
          ],
          "types": [
            "node"
          ],
          "importsNotUsedAsValues": "error",
          "baseUrl": ".",
          "paths": {
            "$components": [
              "src/lib/components"
            ],
            "$components/*": [
              "src/lib/components/*"
            ],
            "$stores": [
              "src/lib/stores"
            ],
            "$stores/*": [
              "src/lib/stores/*"
            ],
            "$modules": [
              "src/lib/modules"
            ],
            "$modules/*": [
              "src/lib/modules/*"
            ],
            "$layout": [
              "src/lib/layout"
            ],
            "$layout/*": [
              "src/lib/layout/*"
            ],
            "$server": [
              "src/lib/server"
            ],
            "$server/*": [
              "src/lib/server/*"
            ],
            "$types": [
              "src/lib/types"
            ],
            "$types/*": [
              "src/lib/types/*"
            ],
            "$utils": [
              "src/lib/utils"
            ],
            "$utils/*": [
              "src/lib/utils/*"
            ],
            "$lib": [
              "src/lib"
            ],
            "$lib/*": [
              "src/lib/*"
            ]
          },
          "skipLibCheck": true
        },
        "include": [
          "./src/**/*.js",
          "./src/**/*.ts",
          "./src/**/*.svelte",
          "./tests/**/*.ts"
        ],
        "exclude": [
          "./node_modules",
          "./svelte-kit/**"
        ]
      }
    }
  }
