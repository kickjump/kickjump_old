{
  "name": "kickjump.com",
  "private": true,
  "description": "kickjump.com website",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s build:\\*",
    "build:css": "run-s css:generate css:minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build",
    "css:generate": "unocss \"app/**/*.{ts,tsx}\" \"../../packages/kickjump__components/src/**/*.{ts,tsx}\" --out-file=app/uno.css",
    "css:minify": "parcel-css --minify --nesting --bundle --sourcemap app/uno.css -o app/uno.css",
    "dev": "NODE_ENV=development npm-run-all build:server --parallel dev:\\*",
    "dev:build": "NODE_ENV=development pnpm build:server --watch",
    "dev:css": "pnpm css:generate --watch",
    "dev:remix": "NODE_ENV=development remix watch",
    "dev:server": "NODE_ENV=development node --inspect --require ./node_modules/dotenv/config ./build/server.js",
    "start": "NODE_ENV=production node ./build/server.js"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.8",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@kickjump/components": "^0.0.1",
    "@kickjump/db": "^0.0.1",
    "@kickjump/tokens": "^0.0.1",
    "@octokit/app": "^12.0.5",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/core": "^3.6.0",
    "@octokit/openapi-types": "^11.2.0",
    "@remix-run/express": "^1.3.5",
    "@remix-run/node": "^1.3.5",
    "@remix-run/react": "^1.3.5",
    "@remix-run/server-runtime": "^1.3.5",
    "@remix-validated-form/with-zod": "^2.0.1",
    "@solana/wallet-adapter-react": "^0.15.4",
    "@solana/web3.js": "^1.37.1",
    "bcryptjs": "^2.4.3",
    "bn.js": "^5.2.0",
    "bs58": "^5.0.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "envsafe": "^2.0.3",
    "express": "^4.17.3",
    "framer-motion": "^6.2.10",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "octokit": "^1.7.1",
    "poly-buffer": "npm:buffer@^6.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.3.1",
    "remix-auth": "^3.2.1",
    "remix-auth-form": "^1.1.1",
    "remix-auth-socials": "^1.0.2",
    "remix-utils": "^3.0.0",
    "remix-validated-form": "^4.3.0",
    "superjson": "^1.8.1",
    "superstruct": "^0.15.4",
    "tiny-invariant": "^1.2.0",
    "ts-extras": "^0.10.2",
    "tweetnacl": "^1.0.3",
    "type-fest": "^2.12.2",
    "zod": "^3.14.4",
    "zod-form-data": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@remix-run/dev": "^1.3.5",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/lodash-es": "^4.17.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "@unocss/cli": "^0.31.0",
    "@unocss/preset-attributify": "^0.31.0",
    "@unocss/preset-icons": "^0.31.0",
    "@unocss/preset-uno": "^0.31.0",
    "@unocss/preset-web-fonts": "^0.31.0",
    "@unocss/reset": "^0.31.0",
    "autoprefixer": "^10.4.4",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.36",
    "msw": "^0.39.2",
    "postcss": "^8.4.12",
    "tsconfig-paths": "^3.14.1",
    "unocss": "^0.31.0"
  },
  "engines": {
    "node": ">=14"
  },
  "monots": {
    "mode": "app",
    "externalModules": [],
    "tsconfigs": {
      "src": false,
      "": {
        "include": [
          "remix.env.d.ts",
          "**/*.ts",
          "**/*.tsx"
        ],
        "compilerOptions": {
          "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
          ],
          "isolatedModules": true,
          "jsx": "react-jsx",
          "module": "CommonJS",
          "moduleResolution": "node",
          "target": "ESNext",
          "baseUrl": ".",
          "paths": {
            "~/*": [
              "./app/*"
            ]
          },
          "skipLibCheck": true,
          "noEmit": true
        }
      }
    }
  }
}
