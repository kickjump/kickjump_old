{
  "name": "kickjump.com",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json"
  },
  "files": [],
  "scripts": {
    "build": "svelte-kit build && pnpm generate:redirects",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-kit dev",
    "generate:intl": "pnpm -w ts ./apps/kickjump.com/generate-intl-types.ts",
    "generate:redirects": "pnpm -w ts ./apps/kickjump.com/generate-redirects.ts",
    "package": "svelte-kit package",
    "prepare": "svelte-kit sync && pnpm generate:intl",
    "preview": "svelte-kit preview",
    "test:playwright": "playwright test"
  },
  "devDependencies": {
    "@chakra-ui/utils": "^1.10.4",
    "@cloudflare/workers-types": "^3.8.0",
    "@felte/validator-zod": "^1.0.8",
    "@formatjs/icu-messageformat-parser": "^2.1.0",
    "@iconify-icons/bi": "^1.2.2",
    "@iconify-icons/ri": "^1.2.1",
    "@iconify-json/logos": "^1.1.5",
    "@iconify/svelte": "^2.2.1",
    "@iconify/types": "^1.1.0",
    "@iconify/utils": "^1.0.32",
    "@kickjump/db": "^0.0.1",
    "@kickjump/svelte-auth": "^0.0.0",
    "@kickjump/tokens": "^0.0.1",
    "@leveluptuts/svelte-element-query": "^1.0.5",
    "@macfja/svelte-persistent-store": "^1.3.0",
    "@octokit/app": "^12.0.5",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/core": "^3.6.0",
    "@octokit/openapi-types": "^11.2.0",
    "@project-serum/anchor": "^0.24.2",
    "@rgossiaux/svelte-headlessui": "^1.0.0-beta.12",
    "@sindresorhus/is": "^4.6.0",
    "@solana/wallet-adapter-base": "^0.9.5",
    "@solana/wallet-adapter-clover": "^0.4.4",
    "@solana/wallet-adapter-coin98": "^0.5.4",
    "@solana/wallet-adapter-glow": "^0.1.1",
    "@solana/wallet-adapter-huobi": "^0.1.0",
    "@solana/wallet-adapter-phantom": "^0.9.3",
    "@solana/wallet-adapter-slope": "^0.5.4",
    "@solana/wallet-adapter-solflare": "^0.6.6",
    "@solana/wallet-adapter-sollet": "^0.11.1",
    "@solana/wallet-adapter-tokenpocket": "^0.4.3",
    "@solana/wallet-adapter-torus": "^0.11.11",
    "@solana/web3.js": "^1.41.3",
    "@sveltejs/adapter-auto": "1.0.0-next.40",
    "@sveltejs/kit": "1.0.0-next.324",
    "@sveltestack/svelte-query": "^1.6.0",
    "@testing-library/svelte": "^3.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/node": "^17.0.30",
    "bcryptjs": "^2.4.3",
    "bn.js": "^5.2.0",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "clsx": "^1.1.1",
    "dotenv": "^16.0.0",
    "fast-sort": "^3.1.3",
    "ismobilejs": "^1.1.1",
    "mdsvex": "^0.10.5",
    "nanoevents": "^6.0.2",
    "octokit": "^1.7.1",
    "precompile-intl-runtime": "^0.4.15",
    "sk-auth": "^0.4.1",
    "solana-credentials": "^0.0.1",
    "superjson": "^1.9.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.7.0",
    "svelte-intl-precompile": "^0.9.0",
    "svelte-preprocess": "^4.10.6",
    "svelte-themes": "^0.0.98",
    "tiny-invariant": "^1.2.0",
    "tiny-rng": "^0.0.5",
    "tinykeys": "^1.4.0",
    "ts-extras": "^0.10.2",
    "tslib": "^2.4.0",
    "tweetnacl": "^1.0.3",
    "type-fest": "^2.12.2",
    "zod": "^3.14.4"
  },
  "monots": {
    "mode": "app",
    "externalModules": [],
    "tsconfigs": {
      "src": false,
      "": {
        "compilerOptions": {
          "noEmit": false,
          "declaration": true,
          "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
          ],
          "preserveValueImports": true,
          "isolatedModules": true,
          "rootDirs": [
            "./",
            "./.svelte-kit/types"
          ],
          "types": [
            "node"
          ],
          "importsNotUsedAsValues": "error",
          "baseUrl": ".",
          "paths": {
            "$components": [
              "src/lib/components"
            ],
            "$components/*": [
              "src/lib/components/*"
            ],
            "$stores": [
              "src/lib/stores"
            ],
            "$stores/*": [
              "src/lib/stores/*"
            ],
            "$layout": [
              "src/lib/layout"
            ],
            "$layout/*": [
              "src/lib/layout/*"
            ],
            "$server": [
              "src/lib/server"
            ],
            "$server/*": [
              "src/lib/server/*"
            ],
            "$types": [
              "src/lib/types"
            ],
            "$types/*": [
              "src/lib/types/*"
            ],
            "$utils": [
              "src/lib/utils"
            ],
            "$utils/*": [
              "src/lib/utils/*"
            ],
            "$lib": [
              "src/lib"
            ],
            "$lib/*": [
              "src/lib/*"
            ]
          },
          "skipLibCheck": true
        },
        "include": [
          "./src/**/*.js",
          "./src/**/*.ts",
          "./src/**/*.svelte"
        ],
        "exclude": [
          "./node_modules",
          "./svelte-kit/**"
        ]
      }
    }
  }
}
