{
  "name": "kickjump.com_",
  "private": true,
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "build": "next build",
    "dev": "next dev",
    "start": "next start"
  },
  "dependencies": {
    "@kickjump/components": "^0.0.1",
    "@kickjump/db": "^0.0.1",
    "@kickjump/tokens": "^0.0.1",
    "@monots/next-plugin": "^0.4.3",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@octokit/app": "^12.0.5",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/core": "^3.6.0",
    "@octokit/openapi-types": "^11.2.0",
    "@prisma/client": "^3.12.0",
    "@s-libs/micro-dash": "^13.1.0",
    "@solana/wallet-adapter-react": "^0.15.4",
    "@trpc/client": "^9.22.0",
    "@trpc/next": "^9.22.0",
    "@trpc/react": "^9.22.0",
    "@trpc/server": "^9.22.0",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "bn.js": "^5.2.0",
    "bs58": "^5.0.0",
    "cookie": "^0.4.2",
    "envsafe": "^2.0.3",
    "gray-matter": "^4.0.3",
    "mdx-bundler": "^9.0.0",
    "next": "^12.1.4",
    "next-auth": "^4.3.1",
    "next-seo": "^5.4.0",
    "next-themes": "^0.1.1",
    "octokit": "^1.7.1",
    "poly-buffer": "npm:buffer@^6.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hot-toast": "^2.2.0",
    "react-query": "^3.34.19",
    "superjson": "^1.8.1",
    "superstruct": "^0.15.4",
    "ts-extras": "^0.10.1",
    "tweetnacl": "^1.0.3",
    "type-fest": "^2.12.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.4",
    "@types/cookie": "^0.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "monots": {
    "tsconfigs": {
      "src": false,
      "": {
        "compilerOptions": {
          "noEmit": true,
          "declaration": false,
          "target": "ESNext",
          "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
          ],
          "module": "CommonJS",
          "allowJs": true,
          "skipLibCheck": true,
          "strict": true,
          "forceConsistentCasingInFileNames": true,
          "esModuleInterop": true,
          "moduleResolution": "node",
          "resolveJsonModule": true,
          "isolatedModules": true,
          "jsx": "preserve",
          "incremental": true,
          "baseUrl": "./src",
          "paths": {
            "~/*": [
              "./*"
            ]
          },
          "types": [
            "node",
            "@testing-library/jest-dom"
          ]
        },
        "include": [
          "next-env.d.ts",
          "**/*.ts",
          "**/*.tsx"
        ],
        "exclude": [
          "node_modules"
        ],
        "ts-node": {
          "transpileOnly": true,
          "compilerOptions": {
            "esModuleInterop": true,
            "target": "ESNext",
            "module": "CommonJS"
          }
        }
      }
    },
    "entrypoints": [
      "index.{ts,tsx}"
    ],
    "mode": "app",
    "externalModules": []
  }
}
